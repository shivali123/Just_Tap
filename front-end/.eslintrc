{
  "env": {
    "browser": true,
    "es6": true,
    "node":true,
    "jest" : true
  },
  "globals": {
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
   },
  "plugins": [
    "prettier",
    "react",
    "jest",
    "json"
  ],
  "rules": {
    "arrow-body-style": 2,
    "block-scoped-var": 2,
    "camelcase": 2,
    "constructor-super": 2,
    "curly": [2, "all"],
    "dot-notation": 2,
    "eqeqeq": 2,
    "linebreak-style": 2,
    "lines-between-class-members": 2,
    "max-depth": 2,
    "new-cap": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-eq-null": 2,
    "no-extra-boolean-cast": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-multi-str": 2,
    "no-octal": 2,
    "no-obj-calls": 2,
    "no-process-env": 0,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 2,
    "no-self-assign": 2,
    "no-sparse-arrays": 2,
    "no-template-curly-in-string": 2,
    "no-undef": 2,
    "no-use-before-define": [ 2, { "functions": false } ],
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-unneeded-ternary": 2,
    "no-unused-vars": 2,
    "no-unreachable": 2,
    "no-unsafe-negation": 2,
    "no-useless-escape": 2,
    "no-useless-return": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-template": 2,
    "radix": 2,
    "react/prop-types": 0,
    "use-isnan": 2,
    "valid-typeof": 2,
    "spaced-comment": 2
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ]
}
